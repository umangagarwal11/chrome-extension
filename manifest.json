{
  "name": "New Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get an image with a greeting whenever you open a new tab.",
  "icons": {
  "16": "icons/icon16.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
  "newtab": "src/override/override.html"
  },
  "content_scripts": [
   {
     "matches": ["https://www.youtube.com/*"],
     "run_at": "document_start",
     "js": ["js/newscript.js"]
   }
 ],
  "permissions": [ "https://source.unsplash.com/","http://quotes.rest/"]
  }
